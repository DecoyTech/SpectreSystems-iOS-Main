<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New HabitProfile</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @habitprofile.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Timedata: </td>
                <td class="itemValue"><input type="text" name="habitprofile[TimeData]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Locationdata: </td>
                <td class="itemValue"><input type="text" name="habitprofile[LocationData]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Actiondata: </td>
                <td class="itemValue"><input type="text" name="habitprofile[ActionData]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Financedata: </td>
                <td class="itemValue"><input type="text" name="habitprofile[FinanceData]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
