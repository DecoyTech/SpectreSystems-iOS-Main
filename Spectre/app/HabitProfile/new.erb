<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @habitprofile.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @habitprofile.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="habitprofile[TimeData]" class="fieldLabel">Timedata</label>
            <input type="text" id="habitprofile[TimeData]" name="habitprofile[TimeData]" <%= placeholder("Timedata") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="habitprofile[LocationData]" class="fieldLabel">Locationdata</label>
            <input type="text" id="habitprofile[LocationData]" name="habitprofile[LocationData]" <%= placeholder("Locationdata") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="habitprofile[ActionData]" class="fieldLabel">Actiondata</label>
            <input type="text" id="habitprofile[ActionData]" name="habitprofile[ActionData]" <%= placeholder("Actiondata") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="habitprofile[FinanceData]" class="fieldLabel">Financedata</label>
            <input type="text" id="habitprofile[FinanceData]" name="habitprofile[FinanceData]" <%= placeholder("Financedata") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
