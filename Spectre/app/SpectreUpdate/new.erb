<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @spectreupdate.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @spectreupdate.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="spectreupdate[Title]" class="fieldLabel">Title</label>
            <input type="text" id="spectreupdate[Title]" name="spectreupdate[Title]" <%= placeholder("Title") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="spectreupdate[Image]" class="fieldLabel">Image</label>
            <input type="text" id="spectreupdate[Image]" name="spectreupdate[Image]" <%= placeholder("Image") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="spectreupdate[Date]" class="fieldLabel">Date</label>
            <input type="text" id="spectreupdate[Date]" name="spectreupdate[Date]" <%= placeholder("Date") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="spectreupdate[Content]" class="fieldLabel">Content</label>
            <input type="text" id="spectreupdate[Content]" name="spectreupdate[Content]" <%= placeholder("Content") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
