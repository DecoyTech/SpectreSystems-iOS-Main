<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @interestprofile.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @interestprofile.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="interestprofile[Category]" class="fieldLabel">Category</label>
            <input type="text" id="interestprofile[Category]" name="interestprofile[Category]" <%= placeholder("Category") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
