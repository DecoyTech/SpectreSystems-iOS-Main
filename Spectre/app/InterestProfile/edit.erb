<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>Edit <%= @interestprofile.Category %></h1>
    <a href="<%= url_for :action => :show, :id => @interestprofile.object %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
    <a href="<%= url_for :action => :delete, :id => @interestprofile.object %>" class="ui-btn-right" data-icon="delete" data-direction="reverse">
      Delete
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @interestprofile.object %>"/>
      
          <div data-role="fieldcontain">
            <label for="interestprofile[Category]" class="fieldLabel">Category</label>
            <input type="text" id="interestprofile[Category]" name="interestprofile[Category]" value="<%= @interestprofile.Category %>" <%= placeholder( "Category" ) %> />
          </div>
      
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
