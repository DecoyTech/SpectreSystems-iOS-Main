<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @time.SpentHere %></h1>
    <%= link_to "Edit", :action => :edit, :id => @time.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">SpentHere</td>
        <td class="itemValue"><%= @time.SpentHere%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">SpentMoving</td>
        <td class="itemValue"><%= @time.SpentMoving%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CurrentTime</td>
        <td class="itemValue"><%= @time.CurrentTime%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CurrentDay</td>
        <td class="itemValue"><%= @time.CurrentDay%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">CurrentDate</td>
        <td class="itemValue"><%= @time.CurrentDate%></td>
      </tr>
      
    </table>
  </div>

</div>
