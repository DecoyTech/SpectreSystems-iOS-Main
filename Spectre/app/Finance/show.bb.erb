        <td class="itemValue"><%= @finance.SpentWhere%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">SpentWhen</td>
        <td class="itemValue"><%= @finance.SpentWhen%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">IncomeTotal</td>
        <td class="itemValue"><%= @finance.IncomeTotal%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">IncomeFrom</td>
        <td class="itemValue"><%= @finance.IncomeFrom%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">BalanceTotal</td>
