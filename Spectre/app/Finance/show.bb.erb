    <%= link_to "Edit", :action => :edit, :id => @finance.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">SpentTotal</td>
        <td class="itemValue"><%= @finance.SpentTotal%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">SpentOn</td>
        <td class="itemValue"><%= @finance.SpentOn%></td>
      </tr>
      
      <tr>
