<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Finance</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @finance.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Spenttotal: </td>
                <td class="itemValue"><input type="text" name="finance[SpentTotal]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Spenton: </td>
                <td class="itemValue"><input type="text" name="finance[SpentOn]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Spentwhere: </td>
                <td class="itemValue"><input type="text" name="finance[SpentWhere]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Spentwhen: </td>
                <td class="itemValue"><input type="text" name="finance[SpentWhen]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Incometotal: </td>
                <td class="itemValue"><input type="text" name="finance[IncomeTotal]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Incomefrom: </td>
                <td class="itemValue"><input type="text" name="finance[IncomeFrom]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Balancetotal: </td>
                <td class="itemValue"><input type="text" name="finance[BalanceTotal]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
